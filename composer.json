{
    "name": "arodu/cakephp-query-filter",
    "description": "QueryFilter plugin for CakePHP 4.x",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "4.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "QueryFilter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "QueryFilter\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
